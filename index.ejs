<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <link rel="shortcut icon" type="image/png" href="./assets/images/favicon.png"/>
    <script type="application/javascript" src="./app/vendor/keymaster.js"></script>
    <title>Balloon Souls</title>
</head>
<body>
<div id="wrapper">
<div id="title-elements">
  <div id="title" class="eightbit" >BALLOON SOULS</div></br>
  <div id="welcome" class="eightbit"><span style="color: white">Any key to start</span>
    </br></br>
      <span>tap "a" to move left : tap "d" to move right</span>
    </br></br>
      <span style="color: red">tap "," to float higher!</span>
  </div>
</div>
<div id="game-contents">
  <div id="ui">
    <div id="score" >SCORE: </div>
    <div id="streak" >BALLOON STREAK: </div>
    <div class="in-line"><div id="hi-score" >HI SCORE:</div><div id="hi-score-val">0</div></div>
  </div>
  <div id="content">
    <canvas id="balloon_souls_canvas"></canvas>
  </div>
</div>
</div>
    <div id=audio-control>
      <div id="audio-toggle">AUDIO: ON</div>
    </div>
    <div id="me">
    <a href="https://www.linkedin.com/in/peter-niemeier-706a362/" target="_blank"><img src="./assets/images/li.png" /></a>
    <a href="https://github.com/peterniemeier/Balloon-Souls" target="_blank"><img src="./assets/images/git.png" /></a>
    </div>
    <!-- <div id="btnStart" class="eightbit">Come get some</div> -->
    <div id="youDied" class="animated fadeIn">
      <span>YOU DIED</span></br>
    <span id="scoreEnd"></span></br>
    <span id="getGoing"></span>
    </div>

    <div id="loading" >
      <span>LOADING</span>
    </div>
    <button type="button" style="z-index:-1;opacity:0;position: absolute;left:-1000px;" id="btn"></button>

</body>
</html>
